	<%- css("js/prism/prism.css")  %>
	<%- css("css/style.css")  %>
	<%- js("https://cdn.staticfile.org/jquery/3.6.4/jquery.min.js") %> 
	<!-- https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.4/jquery.min.js  这是cdnjs的 -->
	<%- js("js/theme.js") %>
