<!-- 文件头 引入css JS -->
<head>
	<meta charset="UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta name="description" content="UnknownMotherGoose">
	<meta name="Keywords" content="UnknownMotherGoose">
	<meta name="Robots" contect= "all">
	<meta name="google-site-verification" content= <%= theme.googlesiteverification %> />
	<title>
		<% if (is_home()) { %>
            <%- config.title %>
        <% } else if (is_archive()) { %>
            <%- __('Archive') %>
        <% } else if (is_category()) { %>
            <%- __('Category') %> - <%- page.category %>
        <% } else if (is_tag()) { %>
            <%- __('Tag') %> - <%- page.tag %>
        <% } else { %>
            <%- page.title +" - " +config.minititle %>
        <% } %>
	</title>
	<link rel="Bookmark" type="image/x-icon" href="<%- url_for(theme.favicon) %>" />
	<link rel="icon" type="image/x-icon" href="<%- url_for(theme.favicon) %>" />
	<link
		rel="shortcut icon"
		type="image/x-icon"
		href="<%- url_for(theme.favicon) %>"
	/>
	<!-- <link rel="stylesheet" href="https://cdn.staticfile.org/lxgw-wenkai-webfont/1.6.0/style.css" /> -->
